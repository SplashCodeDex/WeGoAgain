{
  "mcpServers": {
    "dart": {
      "command": "dart",
      "args": [
        "mcp-server"
      ]
    },
    "Shadcn": {
      "httpUrl": "https://www.shadcn.io/api/mcp",
      "args": []
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "n8n-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "n8n-mcp@2.12.2"
      ],
      "env": {
        "MCP_MODE": "stdio",
        "LOG_LEVEL": "error",
        "DISABLE_CONSOLE_OUTPUT": "true"
      }
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "YOUR_API_KEY"
      ]
    },
    "firebase": {
      "command": "npx",
      "args": [
        "-y",
        "firebase-tools@latest",
        "experimental:mcp",
        "--dir",
        "/home/user/PlanMyLife",
        "--only",
        "auth,firestore,storage"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "Sentry": {
      "url": "https://mcp.sentry.dev/mcp"
    },
    "gitmcp": {
      "url": "https://gitmcp.io/docs"
    }
  },
  "enabledTools": {
    "dart": [
      "connect_dart_tooling_daemon",
      "get_runtime_errors",
      "hot_reload",
      "get_widget_tree",
      "get_selected_widget",
      "set_widget_selection_mode",
      "get_active_location",
      "pub_dev_search",
      "dart_fix",
      "dart_format",
      "run_tests",
      "create_project",
      "pub",
      "analyze_files",
      "resolve_workspace_symbol",
      "signature_help",
      "hover"
    ],
    "Shadcn": [],
    "sequentialthinking": [
      "sequentialthinking"
    ],
    "puppeteer": [
      "puppeteer_navigate",
      "puppeteer_screenshot",
      "puppeteer_click",
      "puppeteer_fill",
      "puppeteer_select",
      "puppeteer_hover",
      "puppeteer_evaluate"
    ],
    "time": [],
    "memory": [
      "create_entities",
      "create_relations",
      "add_observations",
      "delete_entities",
      "delete_observations",
      "delete_relations",
      "read_graph",
      "search_nodes",
      "open_nodes"
    ],
    "n8n-mcp": [],
    "context7": [
      "resolve-library-id",
      "get-library-docs"
    ],
    "firebase": [
      "firebase_login",
      "firebase_logout",
      "firebase_validate_security_rules",
      "firebase_get_project",
      "firebase_list_apps",
      "firebase_list_projects",
      "firebase_get_sdk_config",
      "firebase_create_project",
      "firebase_create_app",
      "firebase_create_android_sha",
      "firebase_get_environment",
      "firebase_update_environment",
      "firebase_init",
      "firebase_get_security_rules",
      "firebase_read_resources",
      "auth_get_users",
      "auth_update_user",
      "auth_set_sms_region_policy",
      "firestore_delete_document",
      "firestore_get_documents",
      "firestore_list_collections",
      "firestore_query_collection",
      "storage_get_object_download_url"
    ],
    "playwright": [],
    "Sentry": [],
